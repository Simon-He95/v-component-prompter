{
  "publisher": "simonhe",
  "name": "v-component-prompter",
  "displayName": "Vue Component Prompter",
  "version": "0.0.45",
  "packageManager": "pnpm@8.15.9",
  "description": "A powerful tool for prompting Vue components with ease.",
  "author": "Simon He <https://github.com/Simon-He95>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/v-component-prompter#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Simon-He95/v-component-prompter"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/v-component-prompter/issues"
  },
  "sponsor": {
    "url": "https://github.com/Simon-He95/sponsor"
  },
  "keywords": [
    "vue",
    "vue-jsx",
    "vue-prompter",
    "vue-components",
    "vue-cli",
    "vue-tools",
    "vue-utilities"
  ],
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.75.1"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "v-component-prompter",
      "properties": {
        "v-component-prompter.code": {
          "type": "string",
          "default": "",
          "description": "activation code"
        }
      }
    },
    "commands": [
      {
        "command": "v-component-prompter.activation",
        "title": "activation v-component-prompter"
      }
    ]
  },
  "scripts": {
    "dev": "pnpm build --watch",
    "test": "vitest",
    "build": "tsup src/index.ts --external vscode",
    "pack": "vsce package --no-dependencies",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix",
    "publish": "vsce publish --no-dependencies",
    "typecheck": "tsc --noEmit",
    "release": "pnpm build && bumpp && pnpm run publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.9.1",
    "@simon_he/fetch-npm-cjs": "^0.0.22",
    "@types/node": "^18.19.64",
    "@types/vscode": "^1.75.1",
    "@vscode-use/utils": "0.1.40",
    "@vscode/vsce": "^3.2.1",
    "bumpp": "^9.8.1",
    "eslint": "^9.14.0",
    "fast-glob": "^3.3.2",
    "find-up": "^7.0.0",
    "lazy-js-utils": "^0.1.28",
    "lazy-parser": "^0.0.8",
    "tsup": "^6.7.0",
    "typescript": "^5.6.3",
    "vitest": "^0.29.8"
  }
}
